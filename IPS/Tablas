CREATE TABLE "EVENTOS"
(
	EV_ID int NOT NULL,
	EV_NOMBRE varchar,
	EV_TIPO varchar,
	EV_DISTANCIA decimal,
	EV_PRECIO decimal,
	EV_FECHA_COMIENZO date,
	EV_FECHA_FIN_INSC date,
	PRIMARY KEY (ID)	
);
CREATE TABLE "ATLETA"
(
	AT_DNI varchar PRIMARy KEY NOT NULL,
	AT_NOMBRE varchar,
	AT_CATEGORIA varchar,
	AT_SEXO int,
	AT_EDAD int,
);
CREATE TABLE "INSCRIPCION"(
	AT_DNI varchar NOT NULL,
	EV_ID varchar NOT NULL,
	INS_ESTADO int,
	INS_FECHA_INS date,
	INS_RESULTADO_SEG int,
	INS_DORSAL int,
	PRIMARY KEY(INS_ATL_DNI, INS_EV_ID),
	FOREIGN(EV_ID) REFERENCES EVENTOS (EV_ID),
	FOREIGN(ATL_DNI) REFERENCES ATLETA (ATL_DNI)
);