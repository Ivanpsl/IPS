CREATE TABLE "EVENTOS"( EV_ID int NOT NULL,
	EV_NOMBRE varchar(255),
	EV_TIPO varchar(255),
	EV_DISTANCIA decimal,
	EV_PRECIO decimal,
	EV_FECHA_COMIENZO date,
	EV_FECHA_FIN_INSC date,
	PRIMARY KEY (EV_ID)
	);
CREATE TABLE "ATLETA"
(
	AT_DNI varchar(255) PRIMARy KEY NOT NULL,
	AT_NOMBRE varchar(255),
	AT_CATEGORIA varchar(255),
	AT_SEXO int,
	AT_EDAD int,
);
CREATE TABLE "INSCRIPCION"(
	AT_DNI  varchar(255) NOT NULL,
	EV_ID int NOT NULL,
	INS_ESTADO int,
	INS_FECHA_INS date,
	INS_RESULTADO_SEG  int,
	INS_DORSAL int,
	PRIMARY KEY(AT_DNI,EV_ID),
	CONSTRAINT FK_EV_ID FOREIGN KEY(EV_ID) REFERENCES PUBLIC."EVENTOS" (EV_ID),
	CONSTRAINT FK_AT_DNI FOREIGN KEY(AT_DNI) REFERENCES PUBLIC."ATLETA" (AT_DNI),
);
CREATE MEMORY TABLE PUBLIC."VENTAS"(CIFC VARCHAR(255),DNI VARCHAR(255),CODCOCHE INTEGER,COLOR VARCHAR(255),CONSTRAINT PK_VENTAS PRIMARY KEY(CIFC,DNI,CODCOCHE),CONSTRAINT FK_VENTAS_CONCESIONARIOS FOREIGN KEY(CIFC) REFERENCES PUBLIC."CONCESIONARIOS"(CIFC),CONSTRAINT FK_VENTAS_CLIENTES 
FOREIGN KEY(DNI) REFERENCES PUBLIC."CLIENTES"(DNI),CONSTRAINT FK_VENTAS_COCHES FOREIGN KEY(CODCOCHE) REFERENCES PUBLIC."COCHES"(CODCOCHE))